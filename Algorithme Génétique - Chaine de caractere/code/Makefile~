CC = g++ -std=c++11
EXEC_NAME = GAstring
OBJ_FILES = genome.o population.o

clean :
	rm $(EXEC_NAME) *.o

$(EXEC_NAME): $(OBJ_FILES)
	$(CC) -o $(EXEC_NAME) $(OBJ_FILES) 

genome.o: genome.cpp
	$(CC) genome.cpp -c 

population.o: population.cpp 
	$(CC) population.cpp -c 

main.o: main.cpp
	$(CC) main.cpp -c 

main: genome.o population.o main.o
	$(CC) genome.o population.o main.o -o $(EXEC_NAME)



